    private void initKeyListeners() {
        for(JTextField textField : locations.keySet()) {
            textField.addKeyListener(this);
        }
    }

    private void clearSudoku() {
        for(JTextField textField : locations.keySet()) {
            textField.setText("");
            textField.setEditable(true);
        }
    }
    
    private void resetSudoku() {
        for(JTextField textField : locations.keySet()) {
            if(textField.isEditable()) {
                textField.setText("");
            }
        }
    }
    
    private void loadSudoku() {
        for(int y = 0 ; y < 9 ; y++) {
            for(int x = 0 ; x < 9 ; x++) {
                Point p = new Point(y, x);
                
                for(Entry<JTextField, Point> entry : locations.entrySet()) {
                    if(p.equals(entry.getValue())) {
                        char c = sudoku.getDataAt(y, x);
                        entry.getKey().setText("" + (c == '0' ? ' ' : c));
                        entry.getKey().setEditable(!sudoku.isFrozenAt(y, x));
                    }
                }
            }
        }
    }
    
    private void showWarning(String message) {
        JOptionPane.showMessageDialog(this, message, 
                "Warning!", JOptionPane.WARNING_MESSAGE);
    }
    
    private void showError(String message) {
        JOptionPane.showMessageDialog(this, message, 
                "Error!", JOptionPane.ERROR_MESSAGE);
    }
    
    private void showSuccess(String message) {
        JOptionPane.showMessageDialog(this, message,
                "Success", JOptionPane.INFORMATION_MESSAGE);
    }
    
    @Override
    public void keyTyped(KeyEvent evt) {
        JTextField source = (JTextField) evt.getSource();
        Point location = locations.get(source);
        
        char c = evt.getKeyChar();

        try {
            if(sudoku.setDataAt(location.x, location.y, c)) {
                source.setText("" + (c == '0' ? ' ' : c));
                
                if(sudoku.isFull()) {
                    showSuccess("Congratulations! You have successfully " +
                            "completed this Sudoku!");
                }
            } else {
                showWarning("Placing " + c + " here would not " +
                        "comply to Sudoku rules!");
            }
        } catch(IllegalArgumentException e) {
            //System.err.println("NOT ALLOWED");
        }
        
        evt.consume();
    }

    @Override
    public void keyPressed(KeyEvent e) {
    }

    @Override
    public void keyReleased(KeyEvent e) {
    }
}
