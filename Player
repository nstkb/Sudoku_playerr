    private void exitMenuItemActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_exitMenuItemActionPerformed
        System.exit(0);
    }//GEN-LAST:event_exitMenuItemActionPerformed

    private void openMenuItemActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_openMenuItemActionPerformed
        fileChooser.setDialogTitle("Open Sudoku");
        
        if(JFileChooser.APPROVE_OPTION == fileChooser.showSaveDialog(this)) {
            try(BufferedReader in = new BufferedReader(
                    new FileReader(fileChooser.getSelectedFile()))) {
                sudoku = new Sudoku(in.readLine());
                loadSudoku();
            } catch(IllegalArgumentException e) {
                showError("Could not open Sudoku! Sudoku file is corrupted!");
            } catch(IOException e) {
                showError("Could not open Sudoku! Possible " +
                        "reason: " + e.getMessage());
            }
        }
    }//GEN-LAST:event_openMenuItemActionPerformed

    private void saveAsMenuItemActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_saveAsMenuItemActionPerformed
        fileChooser.setDialogTitle("Save Sudoku");
        
        if(JFileChooser.APPROVE_OPTION == fileChooser.showSaveDialog(this)) {
            try(FileWriter fileWriter = new FileWriter(
                    fileChooser.getSelectedFile())) {
                fileWriter.write(sudoku.toString());
            } catch(IOException e) {
                showError("Could not save Sudoku! Possible " +
                        "reason: " + e.getMessage());
            }
        }
    }//GEN-LAST:event_saveAsMenuItemActionPerformed

    private void clearMenuItemActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_clearMenuItemActionPerformed
        sudoku = new Sudoku();
        clearSudoku();
    }//GEN-LAST:event_clearMenuItemActionPerformed

    private void resetMenuItemActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_resetMenuItemActionPerformed
        sudoku.clear();
        resetSudoku();
    }//GEN-LAST:event_resetMenuItemActionPerformed

